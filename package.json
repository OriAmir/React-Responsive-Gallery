{
  "name": "react-responsive-gallery",
  "version": "2.1.3",
  "description": "React responsive gallery",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/OriAmir/React-Responsive-Gallery"
  },
  "private": false,
  "keywords": [
    "react",
    "react-gallery",
    "react-responsive-gallery"
  ],
  "scripts": {
    "build": "webpack --mode production --config ./webpack.config.js",
    "build-dev": "webpack --mode development --watch --config ./webpack.config.js",
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest",
    "prettier": "./node_modules/.bin/prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "author": {
    "name": "Ori Amir"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "^26.0.22",
    "@types/shortid": "0.0.29",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "css-loader": "^2.1.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-jest": "^24.3.4",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-app": "^6.2.2",
    "eslint-plugin-testing-library": "^3.10.2",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "prettier": "^2.2.1",
    "prettier-webpack-plugin": "^1.2.0",
    "react-test-renderer": "^16.12.0",
    "sass": "1.49.7",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^2.0.1",
    "style-loader": "^0.23.1",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "nanoid": "^3.1.23",
    "react-image-lightbox": "^5.1.4",
    "styled-components": "^5.3.0"
  }
}
